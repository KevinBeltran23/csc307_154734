{
    "extends": ["eslint:recommended", "plugin:react/recommended"],
    "env": {
        "browser": true,
        "es2023": true,
        "node": true
    },
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "sourceType": "module"
    },
    "rules": {
        "react/prop-types": 0
    },
    "settings": {
        "react": {
            "version": "detect"
        }
    },
    "ignorePatterns": ["**/*.test.js", "**/build/*"]
}
